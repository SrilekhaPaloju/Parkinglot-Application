<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:ndc="sap.ndc"
>
    <Dialog
        id="idAssignedslotsDailog"
        resizable="true"
        draggable="true"
        contentWidth="30%"
        title="Slot Allocation"
        class="dialogSlideDown"
    >
        <!-- <beginButton>
            <Button
                id="_IDAssignButton"
                text="Assign"
                press="onAssignPress"
                type="Accept"
            />
        </beginButton>
        <endButton>
                <Button
                    id="idCancelslot"
                    text="Cancel"
                    press="onCloseCreateDialog"
                    type="Negative"
                />
        </endButton> -->
        <content>
            <form:SimpleForm
                id="SimpleFormToolbar"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="3"
                labelSpanM="4"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="0"
                emptySpanL="4"
                emptySpanM="0"
                emptySpanS="0"
                columnsXL="2"
                columnsL="1"
                columnsM="1"
                singleContainerFullSize="false"
                ariaLabelledBy="Title1"
                width="100%"
            >
                <Label
                    id="_IDGenLabel1"
                    text="Parkinglot Number"
                />
                <!-- <Select
                    id="parkingLotSelect"
                    selectedKey=""
                    items="{path: '/ParkingLot', filters: [{path: 'status', operator: 'EQ', value1: 'Available'}]}"
                >
                    <core:Item
                        id="_IDGenItem4"
                        key="{parkingLotNumber}"
                        text="{parkingLotNumber}"
                    />
                </Select> -->
                <VBox id="idvboxparkingnumber">
                    <ComboBox
                        id="parkingLotSelect"
                        width="50rem"
                        selectedKey="{parkingModel>/parkingLotNumber}"
                        items="{path: '/ParkingLot', filters: [{path: 'status', operator: 'NE', value1: 'Occupied'}]}"
                    >
                        <core:Item
                            id="_IDGenItem3"
                            key="{parkingLotNumber}"
                            text="{parkingLotNumber}"
                        />
                    </ComboBox>
                    <Input
                        id="idparkingLotNumberCol"
                        value=""
                        visible="false"
                    />
                </VBox>

                <Label
                    id="_IDGenLabel2"
                    text="Vehicle Number"
                />
                <HBox>
                    <Input
                        id="_IDGenInput2"
                        value=""
                        width="25rem"
                    />
                    <ndc:BarcodeScannerButton
                        id="sampleBarcodeScannerButtonZebraOne"
                        scanSuccess="onScanSuccessOne"
                        scanFail="onScanErrorOne"
                        inputLiveUpdate="onScanLiveUpdate"
                        dialogTitle=""
                        class="sampleBarcodeScannerButtonWithZebraEB"
                    />
                </HBox>
                <Label
                    id="_IDDriverLabel2"
                    text="Driver Name"
                />
                <Input
                    id="_IDDriverInput2"
                    value=""
                />
                <Label
                    id="_IDPhnnoLabel2"
                    text="Phone Number"
                />
                <Input
                    id="_IDPhnnoInput2"
                    value=""
                />
                <Label
                    id="_IDGenLabel4"
                    text="Transport type"
                />
                <!-- <Select
                    id="country"
                    selectedKey=""
                >
                    <items>
                        <core:Item
                            id="_IDGenItem1"
                            text="Inward"
                            key="Inward"
                        />
                        <core:Item
                            id="_IDGenItem2"
                            text="Outward"
                            key="Outward"
                        />
                    </items>
                </Select> -->
                <ComboBox
                    id="country"
                    selectedKey="{parkingModel>/trasnporTtype}"
                >
                    <items>
                        <core:Item
                            id="_IDGenItem1"
                            text="Inward"
                            key="Inward"
                        />
                        <core:Item
                            id="_IDGenItem2"
                            text="Outward"
                            key="Outward"
                        />
                    </items>
                </ComboBox>
                 <Input
                    id="idIntime"
                    value=""
                    visible="false"
                />
                <Input
                    id="idID"
                    value=""
                    visible="false"
                />
                <Label
                    text="null"
                    visible="false"
                />
                <HBox
                    justifyContent="SpaceBetween"
                    alignItems="Center"
                >
                    <Button
                        id="_IDAssignButton"
                        text="Assign"
                        press="onAssignPress"
                        type="Accept"
                    />
                    <Button
                        id="idUnassignSlot"
                        text="Unassign"
                        press="onUnassignPress"
                        type="Emphasized"
                    />
                    <Button
                        id="idCancelslot"
                        text="Cancel"
                        press="onCloseCreateDialog"
                        type="Negative"
                    />
                </HBox>
            </form:SimpleForm>
        </content>
    </Dialog>
</core:FragmentDefinition>
