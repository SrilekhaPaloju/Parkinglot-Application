<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
>  

<form:SimpleForm
        id="idParkingDetailsForm"
        editable="false"
        layout="ColumnLayout"
        columnsM="2"
        columnsL="3"
        columnsXL="4"
    >
        <Label
            id="idParkingLotNumberLabel"
            text="ParkingLot Number"
        />
        <Text
            id="idparkingLotNumberText"
            text="{parkingLotNumber}"
        />
        <Label
            id="idVehicleNumberLabel"
            text="VehicleNumber"
        />
        <Text
            id="idVehicleNumberText"
            text="{Assignedslots/vehicleNumber}"
        />
        <Label
            id="idDriverNameLabel"
            text="DriverName"
        />
        <Text
            id="idDriverNameText"
            text="{Assignedslots/driverName}"
        />
        <Label
            id="idphoneNumberLabel"
            text="phoneNumber"
        />
        <Text
            id="idphoneNumberText"
            text="{Assignedslots/phoneNumber}"
        />
        <Label
            id="idtrasnporTtypeLabel"
            text="trasnporTtype"
        />
        <Text
            id="idtrasnporTtypeText"
            text="{trasnporTtype}"
        />
         <Label
            id="idinTimeLabel"
            text="inTime"
        />
        <Text
            id="idinTimeText"
            text="{Assignedslots/inTime}"
        />
          <Label
            id="idoutTimeLabel"
            text="outTime"
        />
        <Text
            id="idoutTimeText"
            text="{Assignedslots/outTime}"
        />
          <Label
            id="idstatusLabel"
            text="status"
        />
        <Text
            id="idstatusText"
            text="{status}"
        />
    </form:SimpleForm>
</core:FragmentDefinition>