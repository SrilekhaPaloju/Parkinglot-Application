<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
>

    <VBox id="_IDGenVBoxlot">

        <form:SimpleForm
            id="idParkingDetailsForm"
            editable="{viewModel>/isEditable}"
            layout="ColumnLayout"
            columnsM="2"
            columnsL="3"
            columnsXL="4"
        >
             <Label
                id="idParkingLotNumberLabel"
                text="ParkingLot Number"
            />
            <!-- <ComboBox
                id="parkingLotSelectComboBox"
                selectedKey="{parkinglot_parkingLotNumber}"
                editable="{viewModel>/isEditable}"
                items="{path: '/ParkingLot', filters: [{path: 'status', operator: 'NE', value1: 'Occupied'}]}"
            >
                <core:Item id="IDParkinglotnumgen" key="{parkingLotNumber}"
                        text="{parkingLotNumber}" />
            </ComboBox> -->
            <Text
                id="IDParkinglotnumInput"
                text="{parkinglot/parkingLotNumber}"
            />
            <Label
                id="idVehicleNumberLabel"
                text="Vehicle Number"
            />
            <Text
                id="idVehicleNumberInput"
                text="{vehicleNumber}"
            />
            <Label
                id="idDriverNameLabel"
                text="Driver Name"
            />
            <Text
                id="idDriverNameInput"
                text="{driverName}"
            />
            <Label
                id="idphoneNumberLabel"
                text="Phone Number"
            />
            <Text
                id="idphoneNumberInput"
                text="{phoneNumber}"
            />
            <Label
                id="idtrasnporTtypeLabel"
                text="Transport Type"
            />
            <Text
                id="idtrasnporTtypeInput"
                text="{trasnporTtype}"
            />
            <Label
                id="idinTimeLabel"
                text="In Time"
            />
            <Text
                id="idinTimeText"
                text="{inTime}"
            />
            <Label
                id="idoutTimeLabel"
                text="Out Time"
            />
            <Text
                id="idoutTimeText"
                text=" "
            />
            <Label
                id="idstatusLabel"
                text="Status"
            />
            <Text
                id="idstatusText"
                text="{parkinglot/status}"
            />
        </form:SimpleForm>
    </VBox>
</core:FragmentDefinition>
